.menu-with-lines li {
  position: relative;
  padding-right: 15px;
  margin-right: 5px;
}

.menu-with-lines li:not(:last-child)::after {
  content: "";
  position: absolute;
  right: 0;
  top: 15%;
  height: 70%;
  width: 1px;
  background-color: white;
}

